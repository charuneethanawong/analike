{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_TWELVE_DATA_API_KEY": "@twelve_data_api_key",
    "VITE_FINNHUB_API_KEY": "@finnhub_api_key"
  }
}
